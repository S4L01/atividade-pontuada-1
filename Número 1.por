programa {
  funcao inicio() {
    // variaveis 
     
     inteiro soma, a, b, c 

    // Pergunte os valores

    escreva("\nNúmero A :")
    leia(a)
    limpa()


    escreva("\nNúmero B :")
    leia(b)
    limpa()

    escreva("\nNúmero C =")
